<mvc:View
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:f="sap.f"
    xmlns:core="sap.ui.core"
    xmlns:u="sap.ui.unified"
    xmlns:layout="sap.ui.layout"
    xmlns:l="sap.ui.layout"
    controllerName="BNetSapUI5.Application.Main.views.Prod.controller.Master2">


    <Page showNavButton="true"
    navButtonPress="back" title="Product Catalog">
        <SearchField liveChange="search" value="{/Input}" />


        <List headerText="İlişkili Tablo Listesi" items="{/Lists2}" 
            id="orders2">
            <CustomListItem type="Active" press="handleLoadItems2">
                <VBox class="vbox">
                    <HBox>
                        <Text text="{text}" class="label3"></Text>
                    </HBox>

                </VBox>
            </CustomListItem>
        </List>


    </Page>
</mvc:View>